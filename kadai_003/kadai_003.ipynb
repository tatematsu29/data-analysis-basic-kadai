        "#Numpyのインポート\n",
        "import numpy as np\n",
        "\n",
        "#A,Bの定義\n",
        "A = np.arange(6).reshape(3,2)\n",
        "B = np.arange(8).reshape(2,4)\n",
        "\n",
        "#行列の積\n",
        "C = np.dot(A,B)\n",
        "\n",
        "#結果の表示\n",
        "print(C)\n",
        "\n",
        "#最大値\n",
        "Max = np.max(C)\n",
        "Max"
      ],
